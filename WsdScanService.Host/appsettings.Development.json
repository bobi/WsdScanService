{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "WsdScanService.Discovery": "Trace"
    },
    "Console": {
      "LogLevel": {
        "WsdScanService": "Trace"
      }
    }
  },
  "WsdScanService": {
    "OutputDir": "./tmp/wsdscan",
    "Sane": {
      "UseSaneBackend": true,
      "Device": "pixma:MF220_{Ip}"
    },
    "ScanProfiles": [
      {
        "DisplayName": "Scan to OMC (300dpi)",
        "Resolution": 300
      },
      {
        "DisplayName": "Scan to OMC (600dpi)",
        "Resolution": 600
      }
    ]
  }
}
